{
  "extends": ["tslint:recommended", "tslint-react"],
  "rules": {
    "jsx-no-lambda": false,
    "jsx-no-multiline-js": false,
    "interface-name": [true, "never-prefix"],
    "trailing-comma": false,
    "arrow-parens": false,
    "ordered-imports": false,
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore",
      "allow-pascal-case"
    ]
  }
}
